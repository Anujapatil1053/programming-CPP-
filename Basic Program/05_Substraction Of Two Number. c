#include <iostream>
using namespace std;

//Compiler version g++ 6.3.0

int main()
{   
    int iNo1 = 0,iNo2 = 0 , Sub = 0;
    
    cout << "Enter First Number ="<<endl;
    cin>>iNo1;
    
    cout << "Enter Second Number ="<<endl;
    cin>>iNo2;
    
    Sub = iNo1 - iNo2;
    
    cout<<"Substraction Of " << iNo1 <<" And "<< iNo2 << " = "<< Sub <<endl;
    
    getch();
    return 0;
}
